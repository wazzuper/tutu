<% @wagons.each do |wagon| %>
  <tr>
    <td><%= wagon.number %></td>
    <td><%= wagon.type %></td>
    <td><%= link_to 'Показать', wagon_path(wagon) %></td>
    <td><%= link_to 'Редактировать', edit_wagon_path(wagon) %></td>
    <td><%= link_to 'Удалить', wagon_path(wagon), method: :delete, data: { confirm: 'Вы уверены?' } %></td>
  </tr>
<% end %>